  <!-- INTERSECTION CLASS: BayAreaMajorCity -->
  <rdf:Description rdf:about="http://www.semanticweb.org/california-ontology#BayAreaMajorCity">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Bay Area Major City</rdfs:label>
    <rdfs:comment>Major cities that are located in the Bay Area</rdfs:comment>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.semanticweb.org/california-ontology#MajorCity"/>
          <owl:Restriction>
            <owl:onProperty rdf:resource="http://www.semanticweb.org/california-ontology#locatedIn"/>
            <owl:hasValue rdf:resource="http://www.semanticweb.org/california-ontology#BayArea"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </rdf:Description>

  <!-- UNION CLASS: NorthernOrSouthernCity -->
  <rdf:Description rdf:about="http://www.semanticweb.org/california-ontology#NorthernOrSouthernCity">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Northern or Southern City</rdfs:label>
    <rdfs:comment>Cities located in either Northern or Southern regions</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/california-ontology#City"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Restriction>
            <owl:onProperty rdf:resource="http://www.semanticweb.org/california-ontology#locatedIn"/>
            <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/california-ontology#NorthernRegion"/>
          </owl:Restriction>
          <owl:Restriction>
            <owl:onProperty rdf:resource="http://www.semanticweb.org/california-ontology#locatedIn"/>
            <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/california-ontology#SouthernRegion"/>
          </owl:Restriction>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </rdf:Description>

