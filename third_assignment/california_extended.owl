        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </rdf:Description>

  <!-- PROPERTY RESTRICTION CLASS: EstablishedCity -->
  <rdf:Description rdf:about="http://www.semanticweb.org/california-ontology#EstablishedCity">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
