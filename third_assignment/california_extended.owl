    <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/california-ontology#City"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.semanticweb.org/california-ontology#City"/>
          <owl:Restriction>
            <owl:onProperty rdf:resource="http://www.semanticweb.org/california-ontology#establishedYear"/>
            <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </rdf:Description>

  <!-- PROPERTY RESTRICTION CLASS: CoastalRegion -->
  <rdf:Description rdf:about="http://www.semanticweb.org/california-ontology#CoastalRegion">
